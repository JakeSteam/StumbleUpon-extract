<div id="fb-root"></div>
<script>
//set vars for function

	var suFb = {};

	suFb.source = 'header';
	suFb.key = '8ab252785ffd2ebc69f34b48c78a931d';
	suFb.tld = '.stumbleupon.com';		

	function dofbconnect(){
		FB.init({
			appId: suFb.key, 
			status: true,
			cookie: true, 
			xfbml: true
		});
		
		if (FB._session && FB._session.uid) 
		{
			check_fbuid(FB._session.uid, FB._session.access_token);
		}
		else
		{
			FB.login(null, {perms:'publish_stream,email,user_birthday'});
		};

		FB.Event.subscribe('auth.sessionChange', function(response) {
			if (response.session) 
			{
				check_fbuid(response.session.uid, response.session.access_token);
			}
			else 
			{
				FB.login(null, {perms:'publish_stream,email,user_birthday',display:'dialog'});
			}
		});
	}

	// see if this fbuid is already mapped and redirect accordingly
	function check_fbuid(fbuid, access_token){
		var do_login = 1, provider, version;
		if (typeof(suExtensionApi) != 'undefined')
		{
			suExtensionApi.getProviderInfo(function(info){provider = info.provider; version=info.providerVersion;});
			if (provider == 'mozbar' && parseFloat(version) >= 3.73)
				do_login = 1;
		}
		$.ajax({
			type: "POST",
			url: "/ajax/checkfacebook",
			data: { 
				facebook_connect : fbuid,
				acc_token : access_token,
				login : do_login,
				source: suFb.source 
			},
			dataType: "json",
			success: function(data) {
				if (data.mapping == false)
					window.location.href = 'http://www' + suFb.tld + '/signup.php?pre=fbc_' + suFb.source;
				else
					window.location.href = data.login ? 
						'http://www' + suFb.tld:
						'http://www' + suFb.tld + '/login.php?FBC=1';
			}
		});
	}

</script>

